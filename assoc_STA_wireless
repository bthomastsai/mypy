config wifi-device 'wifi0'
        option type 'qcawificfg80211'
        option macaddr 'cc:be:59:22:22:22'
        option channel 'auto'
        option hwmode '11ac'
        option htmode 'HT80'
        option commitatf '0'
        option burst '1'

config wifi-iface '1_1'
        option device 'wifi0'
        option network 'lan'
        option mode 'ap'
        option encryption 'psk2+aes'
        option key '12345678'
        option ssid 'ehe_5G'
        option macaddr 'cc:be:59:22:22:22'
        option vhtmubfer '1'
        option vhtmubfee '0'
        option mcastenhance '0'
        option blockdfschan '1'
        option ieee80211w '0'
        option ifname 'ath0'
        list wps_config 'push_button'
        list wps_config 'virtual_push_button'
        list wps_config 'physical_push_button'
        option wps_state '2'

config wifi-iface '1_10'
        option device 'wifi0'
        option network 'wifi0'
        option mode 'sta'
        option encryption 'psk2+ccmp'
        option key '63d6340870704476'
        option ssid 'AP1_TT_WPS'
        option macaddr 'd5:be:59:22:22:22'
        option vhtmubfer '0'
        option vhtmubfee '1'
        option mcastenhance '0'
        option blockdfschan '1'
        option ieee80211w '0'
        option ifname 'ath09'
        list wps_config 'push_button'
        list wps_config 'virtual_push_button'
        list wps_config 'physical_push_button'
        option wps_state '2'
        option wds '1'

config wifi-device 'wifi1'
        option type 'qcawificfg80211'
        option macaddr 'cc:be:59:e0:40:53'
        option channel 'auto'
        option txpower '29'
        option hwmode '11axg'
        option htmode 'HT20'
        option commitatf '0'
        option burst '1'

config wifi-iface '2_1'
        option device 'wifi1'
        option network 'lan'
        option mode 'ap'
        option encryption 'psk2+aes'
        option key '12345678'
        option ssid 'ehe'
        option macaddr 'cc:be:59:e0:40:53'
        option vhtmubfer '1'
        option vhtmubfee '0'
        option mcastenhance '0'
        option ieee80211w '0'
        option ifname 'ath1'
        list wps_config 'push_button'
        list wps_config 'virtual_push_button'
        list wps_config 'physical_push_button'
        option wps_state '2'

